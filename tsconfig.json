{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ESNext"],
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
